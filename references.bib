	keywords = {artificial intelligence, development of economics, literature review},
	pages = {1045--1072},
	file = {Full Text PDF:C\:\\Users\\jonas\\Zotero\\storage\\IEEYCFMK\\Lu and Zhou - 2021 - A review on the economics of artificial intelligen.pdf:application/pdf},
